            <%= link_to t('subscription.subscribe_gtin'), "#"%>
            <div class="add-sub"><%= t 'main.products' %></div>

            <div id="profile-news">
                <div class="add-notes">
                    <h4><%= t 'main.add' %> </h4>
                    <%= form_for :subscription, :url => {:action => 'by_gtin', :controller => :subscription,:id=>params[:id]}, :remote => true do |f| %>
                      <%= f.text_area :gtins %>
                      <%= f.submit t('main.apply'), :class => 'submit-add' %>
                        <%#<textarea name="gtins"></textarea>%>
                        <%#<input type="submit" class='submit-add' value="Принять" name="do"/>%>
                    <% end %>
                </div>
            </div>
            <h2><%= t 'main.results' %> :</h2>
            <div id = 'goods-list-short'>
                <div id="gtin_results" class="short-wrap">

                </div>
            </div>
  </div>
</div>

