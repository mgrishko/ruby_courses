%h2 Sign up
= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f|
  = f.error_notification
  .inputs
    = f.input :first_name, :autofocus => true
    = f.input :last_name
    = f.input :email
    = f.input :password
    = f.input :time_zone
  .actions
    = f.button :submit, "Sign up"
= render :partial => "devise/shared/links"
