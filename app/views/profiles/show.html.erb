<div id="left-side">
  <div class="btn-profil">
    <% if current_user.retailer? %>
      <%= render :partial => "clouds" %>
      <%= render :partial => "add_tag" %>
    <% end %>
  </div>
</div>
<div id="middle-side">
  <div class="wrap">
    <div class="address">
      <%= link_to I18n.t('user.title'), '#' %>
    </div>
    <div id="profile-info">
      <div class="info-wrap">
        <h3 class="title"><%= t 'main.information' %></h3>
        <table width='100%' border='0' cellspacing='0' cellpadding='0'>
          <tbody>
            <tr>
              <td><span><%= t 'user.name' %>:</span></td>
              <td><%= @user.name %></td>
            </tr>
            <tr>
              <td><span><%= t 'user.gln' %>:</span></td>
              <td><%= @user.gln %></td>
            </tr>
          </tbody>
        </table>
        </br>
        <% if current_user.retailer? %>
          <%= render :partial => "/user_attributes/user_attributes" %>
        <% end %>
      </div>
    </div>
  </div>
</div>
