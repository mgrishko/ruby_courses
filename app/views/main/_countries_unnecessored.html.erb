<ul>
  <%= render :partial => "country", :collection => @countries %>
</ul>

<script type="text/javascript">
$("li", "#countries").hover(
  function(){
    countrySelect(this);
  },
  function(){
    $(this).removeClass("ac_over");
  }
).click(function(){
  selectCountry(this);
  $(this).addClass("ac_loading");
}
);
</script>