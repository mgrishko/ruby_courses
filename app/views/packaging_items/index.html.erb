<h1>Listing packaging_items</h1>
<table>
  <tr>
  </tr>

<% for packaging_item in @packaging_items %>
  <tr>
    <td><%=h packaging_item.name %>
    <td><%= link_to 'Show', article_packaging_item_path(@article, packaging_item) %></td>
    <td><%= link_to 'Edit', edit_article_packaging_item_path(@article, packaging_item) %></td>
    <td><%= link_to 'Destroy', [@article, packaging_item], :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%=render :partial => "url_bar", :locals => {:article => @article} %>
