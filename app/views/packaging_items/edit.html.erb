<h1><%= t 'edit.base_item' %></h1>

<%= form_for([@base_item, @packaging_item]) do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <p>
    <%= f.submit t('main.update') %>
  </p>
<% end %>
<%= link_to t('main.back'), base_item_packaging_item_path(@base_item, @packaging_item) %>

