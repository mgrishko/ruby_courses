<h1>Поставщики</h1>
<table>
<tr>
<td>GLN</td><td>Название</td><td>количество bi</td><td>дата самого свежего bi</td><td>Подписка</td><td>Instant Subscription</td>
</tr>
<% for u in @users do %>
<tr>
<td><%= u.gln %></td><td><%= u.name %></td><td><%=u.base_items.count %></td><td><%= u.fresh_base_items %></td>
<td>
<button id="bs-<%= u.id %>" onclick="return subscription(this, '<%= u.id %>')"><%= u.my_retailer_info(current_user.id) %></button>
</td>
<td>
<button id="bis-<%= u.id %>" onclick="return instantSubscription(this, '<%= u.id %>')"<% if u.my_retailer_flag(current_user.id) %>disabled="disabled"<% end %>><% if u.my_retailer_flag(current_user.id) %>Недоступно<% else %>Подписаться<% end %></button>
</td>
</tr>
<% end %>
</table>
<%= will_paginate @users %>
