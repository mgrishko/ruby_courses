<ul>
  <%= render :partial => "country", :collection => @countries %>
</ul>

<script type="text/javascript">
$j("li", "#countries").hover(
  function(){
    countrySelect(this);
  },
  function(){
    $j(this).removeClass("ac_over");
  }
).click(function(){
  selectCountry(this);
  $j(this).addClass("ac_loading");
}
);
</script>