<tr id="sr-<%= result.id %>">
	<td>
		<%= link_to result.base_item.gtin, base_item_path(result.base_item, :view => true) %>
	</td>
	<td>
		<%= link_to result.base_item.name, base_item_path(result.base_item, :view => true) %>
	</td>
	<td>
		<%= link_to result.base_item.item.not_new? ? '' : 'new', base_item_path(result.base_item, :view => true) %>
	</td>
	<td>
		<%= link_to truncated_comment(result.base_item), base_item_path(result.base_item, :view => true) %>
	</td>
	<% if result.new? %>
		<td>
			<% remote_form_for(result) do |f| %>
				<%= f.submit "Акцептовать", :name => 'accept' %>
			<% end %>
		</td>
		<td>
			<% remote_form_for(result) do |f| %>
				<%= f.submit "Отменить", :name => 'cancel' %>
			<% end %>
		</td>
	<% else %>
		<td collspan="2"></td>
	<% end %>
</tr>