<% if @subscription.subscription_results.detect {|r| r.new?} %><% remote_form_for :subscription_result, :url => {:action => 'update_all'} do |f| %><input type="hidden" name="subscription_id" value="<%= @subscription.id %>"/><%= f.submit "Акцептовать все", :name => 'accept' %><%= f.submit "Отменить все", :name => 'cancel' %><% end %><% end %>
<%= render :partial => 'result', :collection => @subscription.subscription_results.reverse %>
